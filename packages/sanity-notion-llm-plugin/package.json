{
  "name": "sanity-hermes",
  "version": "0.1.18",
  "description": "A reusable Sanity plugin that connects Notion tables with LLM to generate article drafts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sanity",
    "plugin",
    "notion",
    "llm",
    "ai",
    "content-generation",
    "cms",
    "sanity-plugin"
  ],
  "author": "Nicolai Harms",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nico-Harms/Sanity-Article-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/Nico-Harms/Sanity-Article-plugin/issues"
  },
  "homepage": "https://github.com/Nico-Harms/Sanity-Article-plugin#readme",
  "peerDependencies": {
    "@sanity/ui": "^1.0.0 || ^2.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "sanity": "^3.0.0 || ^4.0.0",
    "styled-components": "^6.0.0"
  },
  "devDependencies": {
    "@sanity/ui": "^1.0.0",
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "sanity": "^4.11.0",
    "styled-components": "^6.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@portabletext/react": "^4.0.3",
    "date-fns": "^4.1.0",
    "sanity-hermes-shared": "^0.1.0"
  }
}
