{
  "name": "sanity-hermes-shared",
  "version": "0.1.0",
  "type": "module",
  "description": "Shared types and constants for the Sanity Notion LLM plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sanity",
    "notion",
    "llm",
    "types",
    "shared"
  ],
  "author": "Nicolai Harms",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nico-Harms/Sanity-Article-plugin.git",
    "directory": "packages/shared"
  },
  "bugs": {
    "url": "https://github.com/Nico-Harms/Sanity-Article-plugin/issues"
  },
  "homepage": "https://github.com/Nico-Harms/Sanity-Article-plugin#readme",
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "default": "./dist/constants/index.js"
    }
  }
}
